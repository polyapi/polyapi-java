{{{typeDeclarations}}}
declare module vari {
  interface {{interfaceName}} {
    {{#each subContexts}}
      {{name}}: {{interfaceName}};
    {{/each}}
    {{#each variableDeclarations}}
      {{#if comment}}
      /**
      {{{comment}}}
      */
      {{/if}}
      {{name}}: {
        get: () => Promise<{{type}}>;
        update: (value: {{type}}) => Promise<void>;
      }
    {{/each}}
  }
}
